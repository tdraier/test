[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Thomas Draier (tdraier)
[access "refs/for/refs/*"]
	read = group user/Thomas Draier (tdraier)
	push = group user/Thomas Draier (tdraier)
	label-Code-Review = -2..+2 group user/Thomas Draier (tdraier)
	label-Verified = -1..+1 group user/Thomas Draier (tdraier)
	rebase = group user/Thomas Draier (tdraier)
	removeReviewer = group user/Thomas Draier (tdraier)
	submit = group user/Thomas Draier (tdraier)
[access "refs/heads/*"]
	read = group user/Thomas Draier (tdraier)
	create = group user/Thomas Draier (tdraier)
	push = +force group user/Thomas Draier (tdraier)
	pushMerge = group user/Thomas Draier (tdraier)
[access "refs/tags/*"]
	push = +force group user/Thomas Draier (tdraier)
	pushTag = group user/Thomas Draier (tdraier)
	pushSignedTag = group user/Thomas Draier (tdraier)
